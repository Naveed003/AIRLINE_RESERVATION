1) CREATE DATABASE EMPLOYEE;
2) USE EMPLOYEE;
3) CREATE TABLE EMPL(EMPNO INT PRIMARY KEY,
        ENAME VARCHAR(10),
        JOB VARCHAR(10)
        ,MGR INT,
        HIREDATE DATE SAL,
        SAL INT,
        COMM INT,
        DEPTNO INT
    );

4) DESC TABLE_NAME;
6) SELECT * FROM EMPL;
7) SELECT EMPNO,ENAME FROM EMPL;
8) SELECT * FROM EMPL WHERE LENGTH(ENAME)<4;
9) SELECT * FROM EMPL WHERE SAL>=25000 AND SAL<=40000;
10) SELECT JOB,SAL FROM EMPL WHER MGR IS NULL;
11) SELECT ENAME FROM EMPL WHERE ENAME LIKE "__A%";
12) SELECT ENAME FROM EMPL WHERE ENAME LIKE "%L%"
13) SELECT DISTINCT JOB FROM EMPL;
14) SELECT SUM(COLUMN_NAME),AVG(COLUMN_NAME),MIN(COLUMN_NAME),MAX(COLUMN_NAME),COUNT(COLUMN_NAME);
15) SELECT UPPER(ENAME),LOWER(ENAME) FROM EMPL
